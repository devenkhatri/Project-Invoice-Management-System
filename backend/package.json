{
  "name": "project-invoice-backend",
  "version": "1.0.0",
  "description": "Backend API for Project Invoice Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "artillery run tests/performance/load-test.yml",
    "test:security": "npm audit && jest --testPathPattern=security",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "sheets": "ts-node src/scripts/init-sheets.ts",
    "sheets:init": "ts-node src/scripts/init-sheets.ts init",
    "sheets:seed": "ts-node src/scripts/init-sheets.ts init --seed",
    "sheets:backup": "ts-node src/scripts/init-sheets.ts backup",
    "sheets:restore": "ts-node src/scripts/init-sheets.ts restore",
    "sheets:validate": "ts-node src/scripts/init-sheets.ts validate",
    "sheets:check": "ts-node src/scripts/init-sheets.ts check"
  },
  "keywords": [
    "project-management",
    "invoice",
    "google-sheets"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@paypal/paypal-server-sdk": "^1.1.0",
    "@types/archiver": "^6.0.3",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.2",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.31.1",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.3",
    "razorpay": "^2.9.6",
    "sharp": "^0.34.3",
    "stripe": "^18.3.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@sentry/node": "^7.91.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "artillery": "^2.0.3",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prom-client": "^15.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0"
  }
}
